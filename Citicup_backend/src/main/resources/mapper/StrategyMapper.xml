<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.citycup.mapper.StrategyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.citycup.entity.Strategy">
        <result column="symbol" property="symbol" />
        <result column="text1" property="text1" />
        <result column="text2" property="text2" />
        <result column="text3" property="text3" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        symbol, text1, text2, text3
    </sql>

</mapper>
