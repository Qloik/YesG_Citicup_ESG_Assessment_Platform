<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.citycup.mapper.CompanyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.citycup.entity.Company">
        <id column="symbol" property="symbol" />
        <result column="name" property="name" />
        <result column="nameF" property="namef" />
        <result column="engname" property="engname" />
        <result column="industry" property="industry" />
        <result column="esg" property="esg" />
        <result column="rank" property="rank" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        symbol, name, nameF, engname, industry, esg, rank
    </sql>

</mapper>
