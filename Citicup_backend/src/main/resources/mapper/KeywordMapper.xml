<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.citycup.mapper.KeywordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.citycup.entity.Keyword">
        <result column="industry" property="industry" />
        <result column="keyword1" property="keyword1" />
        <result column="keyword2" property="keyword2" />
        <result column="keyword3" property="keyword3" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        industry, keyword1, keyword2, keyword3
    </sql>

</mapper>
